<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huailizhi.mapper.OperateLogMapper">

    <select id="getPageResult" resultType="com.huailizhi.pojo.OperateLog">
        select o.*, e.name operateEmpName
        from operate_log o left join emp e on o.operate_emp_id = e.id
    </select>
</mapper>